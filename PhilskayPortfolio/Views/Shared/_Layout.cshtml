  <section class="paralax-mf footer-paralax bg-image sect-mt4 route" style="background-image: url(../../img/overlay-bg.jpg)">
    <div class="overlay-mf"></div>
    <div class="container">
      <div class="row">
        <div class="col-sm-12">
          <div class="contact-mf">
            <div id="contact" class="box-shadow-full">
              <div class="row">
                <div class="col-md-6">
                  <div class="title-box-2">
                    <h5 class="title-left">
                      Send Message 
                    </h5>
                  </div>
                  <div>
                      <form action="javaScript:SendMail()"  role="form" class="contactForm">
                          @if(ViewBag.Message != null)
                           {
                                <div class="alert alert-success">@ViewBag.Message</div>               
                           }
                      <div class="row">
                        <div class="col-md-12 mb-3">
                          <div class="form-group">
                            <input type="text" name="name" asp-for="Name"  class="form-control" required id="name" placeholder="Your Name" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                            <span asp-validation-for="Name" class="text-danger"></span>
                          </div>
                        </div>
                        <div class="col-md-12 mb-3">
                          <div class="form-group">
                            <input asp-for="From" type="email" class="form-control" required  id="email" placeholder="Your Email" data-rule="email" data-msg="Please enter a valid email" />
                            <span asp-validation-for="From" class="text-danger"></span>
                          </div>
                          
                        </div>
                       
                        <div class="col-md-12 mb-3">
                          <div class="form-group">
                            <textarea class="form-control" name="message" asp-for="Message" required rows="5" 
                            data-rule="required" id="message" data-msg="Please write something for us" 
                            placeholder="Message">
                        </textarea>
                            <span asp-validation-for="Message" class="text-danger"></span>
                          </div>
                        </div>
                        <div class="col-md-12">
                          <button type="submit" class="button button-a button-big button-rouded">Send Message</button>
                        </div>
                      </div>
